# DEFAULT CACHE REGION
jcs.default=DC
jcs.default.cacheattributes=org.apache.jcs.engine.CompositeCacheAttributes
jcs.default.cacheattributes.MaxObjects=5000
jcs.default.cacheattributes.MemoryCacheName=org.apache.jcs.engine.memory.lru.LRUMemoryCache
jcs.default.cacheattributes.UseMemoryShrinker=true
jcs.default.cacheattributes.ShrinkerIntervalSeconds=300
jcs.default.cacheattributes.MaxMemoryIdleTimeSeconds=1800
jcs.default.elementattributes=org.apache.jcs.engine.ElementAttributes
jcs.default.elementattributes.IsEternal=false
jcs.default.elementattributes.MaxLifeSeconds=3600
jcs.default.elementattributes.IdleTime=1800
jcs.default.elementattributes.IsSpool=true
jcs.default.elementattributes.IsLateral=false
jcs.default.elementattributes.IsRemote=false

jcs.auxiliary.DC=org.apache.jcs.auxiliary.disk.indexed.IndexedDiskCacheFactory
jcs.auxiliary.DC.attributes=org.apache.jcs.auxiliary.disk.indexed.IndexedDiskCacheAttributes
jcs.auxiliary.DC.attributes.DiskPath=/data/tomcat_jcs_cache_data
jcs.auxiliary.DC.attributes.maxKeySize=100000

jcs.region.main_cache=DC
jcs.region.main_cache.cacheattributes=org.apache.jcs.engine.CompositeCacheAttributes
jcs.region.main_cache.cacheattributes.MaxObjects=10000
jcs.region.main_cache.cacheattributes.MemoryCacheName=org.apache.jcs.engine.memory.lru.LRUMemoryCache
jcs.region.main_cache.cacheattributes.UseMemoryShrinker=true
jcs.region.main_cache.cacheattributes.ShrinkerIntervalSeconds=300
jcs.region.main_cache.cacheattributes.MaxMemoryIdleTimeSeconds=1800
jcs.region.main_cache.cacheattributes.MaxSpoolPerRun=500
jcs.region.main_cache.elementattributes.IsLateral=false
jcs.region.main_cache.elementattributes.IsRemote=false
jcs.region.main_cache.elementattributes=org.apache.jcs.engine.ElementAttributes
jcs.region.main_cache.elementattributes.IsEternal=false
